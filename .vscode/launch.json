{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Desafio_CRUD_Plataforma-app",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/desafio-crud-plataforma-approuter/node_modules/@sap/html5-repo-mock/index.js",
      "args": [
        "--standalone",
        "/desafiol4eapp/index.html",
        "--ui5",
        "{\"version\":\".\"}"
      ],
      "cwd": "${workspaceFolder}/app",
      "env": {
        "PORT": "6004",
        "MOCK_LOOKUP_DIRS": "[\"webapp\"]",
        "run.config": "{\"handlerId\":\"ui5_run_config_handler_id\",\"runnableId\":\"/home/user/projects/Desafio_CRUD_Plataforma/app\"}"
      },
      "envFile": "${workspaceFolder}/app/.env1",
      "preLaunchTask": "Build Desafio_CRUD_Plataforma"
    }
  ]
}